<Window x:Class="XmlDocConverter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XmlDocConverter"
        mc:Ignorable="d"
        Title="XML Documentation Converter" Height="450" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Frame x:Name="FileSelectionFrame" NavigationUIVisibility="Hidden"/>
        </StackPanel>
        <Frame x:Name="DocumentationViewerFrame" NavigationUIVisibility="Hidden"/>
    </DockPanel>
</Window>